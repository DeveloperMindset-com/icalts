{
  "name": "icalts",
  "version": "0.0.2",
  "description": "iCal parser written in pure typescript",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "scripts": {
    "docs": "npx typedoc --plugin typedoc-plugin-markdown",
    "dev": "npx ts-node-dev src/index.ts",
    "tslint": "npx tslint -p tsconfig.json",
    "tslint-fix": "npx tslint --fix -p tsconfig.json",
    "test": "nyc jest",
    "posttest": "nyc report --reporter=json",
    "prepare": "npm run test",
    "prepublishOnly": "npm run test && npx tsc"
  },
  "keywords": [
    "ical",
    "parser",
    "typescript",
    "ics",
    "calendar"
  ],
  "author": "Eugene Hauptmann",
  "repository": {
    "type": "git",
    "url": "https://github.com/eugenehp/icalts.git"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^25.0.3",
    "codecov": "^3.8.3",
    "jest": "^30.0.5",
    "nyc": "^17.0.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tslib": "^2.1.0",
    "tslint": "^6.1.3",
    "typedoc": "^0.28.1",
    "typedoc-plugin-markdown": "^4.0.3",
    "typescript": "^5.7.3"
  }
}
